{
    "type": "origins:multiple",
    "levitation": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:apply_effect",
                    "effect": {
                        "effect": "minecraft:jump_boost",
                        "duration": 20,
                        "amplifier": 2,
                        "show_icon": false,
                        "show_particles": false
                    }
                },
                {
                    "type": "origins:apply_effect",
                    "effect": {
                        "effect": "minecraft:slow_falling",
                        "duration": 20,
                        "amplifier": 0,
                        "show_icon": false,
                        "show_particles": false
                    }
                }
            ]
        },
        "interval": 15,
        "hud_render": {
            "should_render": false
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "*:*_toggle"
                }
            ]
        }
    },
    "sound": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:power_active",
                "power": "*:*_toggle"
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:play_sound",
                        "sound": "minecraft:block.fire.ambient",
                        "volume": 0.5,
                        "pitch": 1
                    },
                    {
                        "type": "origins:execute_command",
                        "command": "title @s actionbar {\"text\":\"STAR BOOST ON\",\"color\":\"green\"}",
                        "permission_level": 4
                    }
                ]
            },
            "else_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:play_sound",
                        "sound": "minecraft:block.fire.ambient",
                        "volume": 0.5,
                        "pitch": 0.75
                    },
                    {
                        "type": "origins:execute_command",
                        "command": "title @s actionbar {\"text\":\"STAR BOOST OFF\",\"color\":\"red\"}",
                        "permission_level": 4
                    }
                ]
            }
        },
        "key": {
            "key": "key.origins.secondary_active"
        },
        "cooldown": 1,
        "hud_render": {
            "should_render": false
        }
    },
    "toggle": {
        "type": "origins:toggle",
        "active_by_default": false,
        "key": {
            "key": "key.origins.secondary_active"
        }
    },
    "name": "Star Boost §f§u(2)",
    "description": "Activate to increase your jump height and gain slow falling."
}